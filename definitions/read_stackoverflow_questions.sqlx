WITH Sequences AS
  (
    SELECT 
    id,
    split(tags, "|") as tag,
    FORMAT_DATE('%Y-%m', date(creation_date)) as date
    FROM `bigquery-public-data.stackoverflow.posts_questions` 
  )
SELECT flattened_tags, date, count(*) as count
FROM Sequences
CROSS JOIN UNNEST(Sequences.tag) AS flattened_tags
GROUP BY flattened_tags, date
ORDER BY date DESC, count DESC